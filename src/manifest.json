{
  "manifest_version": 2,
  "name": "Research Bookmarker",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Bookmark to Research repo",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab","tabs"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
